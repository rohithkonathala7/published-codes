-- ALTER TABLE
--   Employee
-- ADD
--   salary integer;
-- ***************************************************
-- SELECT
--   *
-- FROM
--   Employee
-- WHERE
--   name = "john";
-- ***************************************************
-- UPDATE
--   Employee
-- SET
--   salary = 700000
-- WHERE
--   name = "rohith";
-- ************************************************
-- SELECT
--   *
-- FROM
--   Employee
-- WHERE
--   name IS NULL
--   AND salary = 700000
-- *********************************************
-- DELETE FROM
--   Employee
-- WHERE
--   name IS NULL
--   AND salary = 700000
-- *********************************************
-- To get duplicate rows
-- *********************************************
-- SELECT
--   name,
--   id,
--   designation,
--   salary,
--   ROW_NUMBER() over (
--     PARTITION by name,
--     id,
--     designation,
--     salary
--     ORDER BY
--       name,
--       id,
--       designation,
--       salary
--   ) rownum
-- FROM
--   Employee;
--   ***********************************
WITH T AS (
  SELECT
    name,
    id,
    designation,
    salary,
    ROW_NUMBER() over (
      PARTITION by name,
      id,
      designation,
      salary
      ORDER BY
        name,
        id,
        designation,
        salary
    ) rownum
  FROM
    Employee
)
DELETE FROM
  T
WHERE
  (rownum > 1)
